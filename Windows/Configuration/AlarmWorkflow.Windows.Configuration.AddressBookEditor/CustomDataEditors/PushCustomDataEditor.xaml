<UserControl x:Class="AlarmWorkflow.Windows.Configuration.AddressBookEditor.CustomDataEditors.GrowlCustomDataEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        
        <ComboBox x:Name="cboConsumer" Grid.Column="0" Margin="2" IsEditable="True" ToolTip="Push-Empfängerplugin" Width="75" />
        <TextBox x:Name="txtRecipientApiKey" Grid.Column="1" Margin="2" ToolTip="API-Key des Empfängers" />
    </Grid>
</UserControl>
